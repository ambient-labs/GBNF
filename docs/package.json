{
  "name": "docs",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wireit",
    "build": "wireit",
    "build:py": "wireit"
  },
  "wireit": {
    "build:py": {
      "command": "cd ../packages/gbnf/python && make build_package"
    },
    "dev": {
      "command": "docoddity dev --port 6166",
      "dependencies": [
        "../packages/gbnf/javascript:build",
        "build:py"
      ]
    },
    "build": {
      "command": "rimraf build && docoddity build --buildDir build -v ./docoddity.vite.config.js",
      "dependencies": [
        "../packages/gbnf/javascript:build",
        "build:py"
      ]
    }
  },
  "author": "Ambient Labs",
  "license": "MIT",
  "dependencies": {
    "@emotion/css": "^11.13.4",
    "@shoelace-style/shoelace": "^2.17.1",
    "copy-component": "^0.3.0",
    "docoddity": "^0.4.14",
    "gbnf": "workspace:*",
    "js-yaml": "^4.1.0",
    "lit": "^3.2.0",
    "pyodide": "^0.26.2",
    "shiki": "^1.21.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "toml": "^3.0.0",
    "wireit": "^0.14.9"
  }
}
